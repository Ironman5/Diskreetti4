
function A = addParent(A,root)
    
    if (A(root,1)== -1) && (A(root,2)== -1)
        A(root,3) = root;
    else
        A(root,3) = root;
        A = addParent(A,(A(root,1)));
        A = addParent(A,A(abs(root,2)));
    end
    
