%         A = addParent(A,A(root,1));
%     elseif A(root,2)~= -1 
%         A(root,3) = root;
%         A = addParent(A,A(root,2));
%     else 
%         A(root,3) = root;        
%     end
    
%A = randBST(10)
%R = rotations01(A,4)